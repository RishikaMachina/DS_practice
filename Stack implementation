class stack(object):
    
    def __init__(self):
        self.items = []
        
    def size(self):
        return len(self.items)
    
    def isEmpty(self):
        return self.items == []
    
    def push(self,x):
        return self.items.append(x)
    
    def pop(self):
        return self.items.pop()
    
    def peek(self):
        return self.items[-1]
