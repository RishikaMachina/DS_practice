class Node(object):
    
    def __init__(self,value):
        
        self.value = value
        self.nextNode = None
        
def last_nth_node(n,head):
    p = head
    q = head.nextNode
    for i in range(0,n-1):
        q = q.nextNode
  #    print("q value incrementing")
    while p is not None:
        if q is None:
            return p.value
        else:
            p = p.nextNode
            q = q.nextNode
    pass


a = Node(1)
b= Node(2)
c = Node(3)
d = Node(4)
e= Node(5)

a.nextNode = b
b.nextNode = c
c.nextNode = d
d.nextNode = e
e.nextNode = None


print(last_nth_node(1,a))
print(last_nth_node(2,a))
print(last_nth_node(3,a))
print(last_nth_node(4,a))
print(last_nth_node(5,a))
