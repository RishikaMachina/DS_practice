class deque(object):
    
    def __init__(self):
        self.items = []
        
    def isEmpty(self):
        return self.items == []
    
    def size(self):
        return len(self.items)
    
    def addFront(self,x):
        return self.items.insert(0,x)
    
    def addRear(self,x):
        return self.items.append(x)
    
    def removeFront(self):
        return self.items.pop(0)
    
    def removeRear(self):
        return self.items.pop()
        
    def size(self):
        return len(self.items)
    
    def frontPeek(self):
        return self.items[0]
    
    def rearPeek(self):
        return self.items[-1]
