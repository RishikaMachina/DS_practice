    "a = []\n",
    "\n",
    "def reverse(s):\n",
    "    if ( len(s) == 0 ):\n",
    "        return\n",
    "    \n",
    "    a.append(s[-1])\n",
    "    reverse(s[0:len(s)-1])\n",
    "    p = ' '.join(a)\n",
    "    return p"
   ]
